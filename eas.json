{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SENTRY_DSN": "",
        "SENTRY_ORG": "",
        "SENTRY_PROJECT": "",
        "SENTRY_AUTH_TOKEN": ""
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_SENTRY_DSN": "",
        "SENTRY_ORG": "",
        "SENTRY_PROJECT": "",
        "SENTRY_AUTH_TOKEN": ""
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6755243828",
        "bundleIdentifier": "com.volvoxllc.twelvesteptracker"
      }
    }
  }
}
