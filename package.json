  },
  "bugs": {
    "url": "https://github.com/billchirico/sobriety-waypoint/issues"
  },
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "ios": "EXPO_NO_TELEMETRY=1 expo run:ios",
    "android": "EXPO_NO_TELEMETRY=1 expo run:android",
    "web": "EXPO_NO_TELEMETRY=1 expo run:web",
    "build:android": "expo export --platform android",
    "build:ios": "expo export --platform ios",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --maxWorkers=2",
    "test:coverage": "jest --coverage",
    "maestro": "maestro test .maestro/flows",
    "maestro:record": "maestro record",
    "prepare": "husky"
  },
